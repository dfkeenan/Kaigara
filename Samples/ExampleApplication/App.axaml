<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:k="https://github.com/dfkeenan/Kaigara"
             x:Class="ExampleApplication.App"
             RequestedThemeVariant="Dark">
    <Application.DataTemplates>
        <k:ShellViewLocator></k:ShellViewLocator>
    </Application.DataTemplates>
    <Application.Resources>
      <ResourceDictionary>
        <ResourceDictionary.MergedDictionaries>
          <ResourceInclude Source="avares://VisualStudioImageLibrary.Avalonia/Assets/Icons.axaml"/>
        </ResourceDictionary.MergedDictionaries>
      </ResourceDictionary>
    </Application.Resources>
    <Application.Styles>
        <SimpleTheme  />
        <k:SimpleShellTheme />
    </Application.Styles>
</Application>
